+class LoginView(View):
+    def get(self,request):
+        return render(request,'login.html')
+    def post(self,request):
+        #1、接收参数
+        #2、参数的验证
+        #2、1验证手机是否符合规则
+        #2、2验证密码是否符合规则
+        #3、状态的保持
+        #4、根据用户选择的是否记住登录记性判断
+        #5、为了首页显示我们需要一些cookiess设置
+        mobile=request.POST.get('mobile')
+        password=request.POST.get('password')
+        #默认remember是on
+        remember=request.POST.get('remember')
+        if not re.match(r'^1[3-9]\d{9}$',mobile):
+            return HttpResponseBadRequest("手机号码不符合")
+        if not re.match(r'^[a-zA-Z0-9]{8,20}$',password):
+            return HttpResponseBadRequest("密码不符合规范")
+
+        #默认是username验证，当前传递的是mobile，所以验证是失败的
+        user=authenticate(mobile=mobile,password=password)
+        if user is None:
+            return HttpResponseBadRequest("用户名或者密码错误")
+
+        response=redirect(reverse('home:index'))
+        if remember !='on':
+            #浏览器关闭之后
+            #设置过期时间
+            #o表示浏览器关闭之后
+            request.session.set_expiry(0)
+            response.set_cookie('is_login',user.username,max_age=14*14*3600)
+            pass
+        else:
+            #默认的是记住默认2周
+            request.session.set_expiry(None)
+            response.set_cookie('is_login',True,max_age=14*24*3600)
+            response.set_cookie('username',user.username,max_age=14*24*3600)
+            pass
+
+        #返回响应
+        return response
